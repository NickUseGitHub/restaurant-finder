{
  "env": {
    "es6": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "globals": {
    "console": "readonly",
    "module": "readonly",
    "process": "readonly",
    "require": "readonly"
  },
  "plugins": ["prettier"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "semi": "error",
    "prettier/prettier": "error"
  }
}
